{
  "name": "axel",
  "version": "0.0.1",
  "description": "mtg simulator",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "setup:local": "node ./dist/scripts/setUpLocalEnv.js",
    "setup:local:ts": "TS_NODE_FILES=true ts-node ./src/scripts/setUpLocalEnv.ts",
    "start": "TS_NODE_FILES=true ts-node ./src/index.ts",
    "start:prod": "node ./dist/index.js",
    "test": "jest"
  },
  "keywords": [
    "axel",
    "mtg"
  ],
  "author": "Roberto Pintos LÃ³pez",
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.12",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.3.0",
    "http-status-codes": "^2.1.2",
    "inversify": "^5.0.1",
    "mongoose": "^5.10.2",
    "reflect-metadata": "^0.1.13"
  }
}
