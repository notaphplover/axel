{
  "name": "axel",
  "version": "0.0.1",
  "description": "mtg simulator",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "setup:local": "node ./dist/scripts/setUpLocalEnv.js",
    "setup:local:ts": "TS_NODE_FILES=true ts-node ./src/scripts/setUpLocalEnv.ts",
    "start": "TS_NODE_FILES=true ts-node ./src/index.ts",
    "start:prod": "node ./dist/index.js",
    "test": "jest",
    "test:integration": "jest --selectProjects Integration",
    "test:unit": "jest --selectProjects Unit"
  },
  "keywords": [
    "axel",
    "mtg"
  ],
  "author": "Roberto Pintos LÃ³pez",
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "typescript-json-schema": "^0.43.0"
  },
  "dependencies": {
    "ajv": "^6.12.5",
    "argon2": "^0.27.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.4.1",
    "http-status-codes": "^2.1.3",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.5",
    "reflect-metadata": "^0.1.13"
  }
}
